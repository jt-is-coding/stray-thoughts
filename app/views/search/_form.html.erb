<%= form_with(url: "/search", method: :get, class: "nav-link form-inline") do |form| %>
  <div class="d-flex">
    <%= form.text_field :year, placeholder: "Input a year!", class: "form-control mr-sm-2" %>
    <%= form.submit "Search", class: "btn btn-outline-success my-2 my-sm-0" %>
  </div>
<% end %>
